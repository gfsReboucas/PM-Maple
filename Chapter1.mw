<Output><Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display="">NiUqJkkoZXBzaWxvbkc2IiIiIiZJInVHRiU2I0YmRiYqJkYkIiIjJkYoNiNGK0YmKiZGJCIiJCZGKDYjRi9GJg==</Equation></Text-field>
</Output>
</Group>
<Group hide-input="false" labelreference="L30" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Text" bold="true" foreground="[255,0,0]" executable="true" family="Monospaced" layout="Normal"><Font bold="true" foreground="[255,0,0]" family="Monospaced" executable="true">seq('f'(i),i=timeScales);</Font></Text-field>
