<Group hide-input="false" hide-output="false" labelreference="L64" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Text" bold="true" foreground="[255,0,0]" executable="true" family="Monospaced" layout="Normal"><Font bold="true" foreground="[255,0,0]" family="Monospaced" executable="true">sol:=u(t)=subs(sol0,sol1,uExp(1,t));</Font></Text-field>
